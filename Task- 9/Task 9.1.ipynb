    "def solution(vals):\n",
    "    cnt = 0 \n",
    "    for i in range(len(vals)):\n",
    "        rang = i + vals[i]\n",
    "        for j in range(i+1, len(vals)):\n",
    "            if rang >= j-vals[j]:\n",
    "                cnt +=1\n",
    "    return cnt"
   ]
